{
  "name": "budget-ninja",
  "version": "0.1.0",
  "description": "My simple redux budgeting application.",
  "engines": {
    "node": "5.9.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server.dev.js",
    "lint": "eslint src",
    "clean": "cross-env NODE_ENV=production rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "start": "cross-env NODE_ENV=production node server.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "postinstall": "npm run build:webpack"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:StephenSmithwick/budget-ninja.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/tsaiDavid/simple-redux-boilerplate",
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.16.1",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "rimraf": "^2.4.3",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-eslint": "^5.0.0-beta4"
  },
  "dependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.3.15",
