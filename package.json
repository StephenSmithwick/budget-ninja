    "moment": "2.13.0",
    "node-sass": "3.7.0",
    "normalize.css": "4.1.1",
    "react": "^15.0.1",
    "react-addons-css-transition-group": "15.0.1",
    "react-addons-test-utils": "15.0.1",
    "react-dom": "15.0.1",
    "react-redux": "^4.1.1",
    "react-tap-event-plugin": "^1.0.0",
    "react-toolbox": "0.16.2",
